@model IEnumerable<SomerenApp.Models.Room>

<h2>Rooms List</h2>

<table>
    <tr>
        <th>Room Number</th>
        <th>Room Size</th>
        <th>Room Type</th>
    </tr>
    @foreach (var room in Model)
    {
        <tr>
            <td>@room.RoomNumber</td>
          
            <td>@room.RoomType</td>
        </tr>
    }
</table>

